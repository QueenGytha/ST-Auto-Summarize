{
  "mcpServers": {
    "playwright": {
      "type": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@playwright/mcp@latest",
        "--browser",
        "chrome"
      ],
      "env": {}
    },
    "fetch": {
      "type": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "bash",
        "-lc",
        "uvx mcp-server-fetch --ignore-robots-txt"
      ],
      "env": {}
    },
    "sequential-thinking": {
      "type": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "env": {}
    }
  }
}
