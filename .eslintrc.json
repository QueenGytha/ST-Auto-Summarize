{
  "extends": [
    "../../../../../.eslintrc.cjs"
  ],
  "plugins": [
    "import"
  ],
  "env": {
    "browser": true
  },
  "globals": {
    "localStorage": "readonly"
  },
  "rules": {
    "import/named": "error",
    "import/no-unresolved": ["error", {
      "ignore": ["^\\.\\.\\.\\.\\./"]
    }]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js"]
      }
    }
  }
}
